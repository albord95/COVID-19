   "metadata": {},
   "outputs": [],
   "source": [
    "def powerlaw(x, amplitude, exponent):\n",
    "    return amplitude * x**(exponent)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def exponential(x, amplitude, tau):\n",
    "    return amplitude * np.exp(x/tau)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def gauss(x, amplitude, mean, sigma):\n",
    "    return amplitude*np.exp(-(x-mean)**2/(2*sigma**2))"
   ]
  },
